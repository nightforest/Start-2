		<![endif]-->
	body
		// Preloader 
		#preloader
			.cssload-whirlpool
		
		// Navbar 
		nav.navbar.navbar-default
			.container
				.navbar-header
					button.navbar-toggle.collapsed(type="button", data-toggle="collapse", data-target="#bs-example-navbar-collapse-1", aria-expanded="false")
						span.sr-only Toggle navigation
						span.icon-bar.top-bar
						span.icon-bar.middle-bar
						span.icon-bar.bottom-bar
					a.navbar-brand(href="#") Website.com
				#bs-example-navbar-collapse-1.collapse.navbar-collapse
					ul.nav.navbar-nav
						li.active
							a(href="#")
								| Link
								span.sr-only (current)
						li
							a(href="#") Link
						li.dropdown
							a.dropdown-toggle(href="#", data-toggle="dropdown", role="button", aria-haspopup="true", aria-expanded="false") Drop down1
							ul.dropdown-menu
								li
									a(href="#") Action
								li
									a(href="#") Another action
								li.dropdown.dropdown-submenu
									a.dropdown-toggle(href="#", data-toggle="dropdown") Something else here
									ul.dropdown-menu
										li
											a(href="#") Dropdown Submenu Link 4.1
										li
											a(href="#") Dropdown Submenu Link 4.2
										li
											a(href="#") Dropdown Submenu Link 4.3
										li
											a(href="#") Dropdown Submenu Link 4.4
								li
									a(href="#") Separated link

								li.dropdown.dropdown-submenu
									a.dropdown-toggle(href="#", data-toggle="dropdown") Dropdown Link 4
									ul.dropdown-menu
										li
											a(href="#") Dropdown Submenu Link 4.1
										li
											a(href="#") Dropdown Submenu Link 4.2
										li
											a(href="#") Dropdown Submenu Link 4.3
										li
											a(href="#") Dropdown Submenu Link 4.4
						li
							a(href="#") Link
					
					ul.nav.navbar-nav.navbar-right
						li.dropdown
							a.dropdown-toggle(href="#", data-toggle="dropdown", role="button", aria-haspopup="true", aria-expanded="false") Dropdown1
							ul.dropdown-menu
								li
									a(href="#") Action
								li
									a(href="#") Another action
								li
									a(href="#") Something else here
								li
									a(href="#") Separated link

								li.dropdown.dropdown-submenu.left
									a.dropdown-toggle(href="#", data-toggle="dropdown") Dropdown Link 4
									ul.dropdown-menu
										li
											a(href="#") Dropdown Submenu Link 4.1
										li
											a(href="#") Dropdown Submenu Link 4.2
										li
											a(href="#") Dropdown Submenu Link 4.3
										li
											a(href="#") Dropdown Submenu Link 4.4
		
		
		// Hidden Popup 
		.hidden
			

		// JS 
		// build:js js/all.min.js 
		script(src='js/jquery.min.js')
		script(src='js/bootstrap.min.js')
		script(src='js/common.js')
		// endbuild 